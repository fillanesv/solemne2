!R6
ena 
conf t
ipv unicast
host R6

int lo0
	ip add 6.0.0.1 255.0.0.0
	no sh
exit

int se0/0/0
	encapsulation frame-relay ietf
	frame-relay lmi-type ansi
	no sh
exit

int se0/0/0.65 point-to-point
	ip add 192.168.56.6 255.255.255.0 
	ipv add 2015:56::6/64
	frame-relay interface-dlci 65
	ipv ospf 200 area 0
	ip nat outside
exit

int g0/0
	no sh
exit

int g0/0.60
	encapsulation dot1Q 60
	ip add 172.16.60.5 255.255.255.0
	ip nat inside
exit

int g0/0.61
	encapsulation dot1Q 61
	ipv ena
	ipv add 2011:61::6/64
	ipv ospf 200 area 0
exit


ip dhcp excluded-address 172.16.60.1 172.16.60.10
ip dhcp pool VLAN60
	network 172.16.60.0 255.255.255.0
	default-router 172.16.60.5
exit

router ospf 100
	router-id 6.6.6.6
	network 172.16.60.0 0.0.0.255 area 0
	network 192.168.56.0 0.0.0.255 area 0
	network 6.0.0.0 0.255.255.255 area 0
	passi g0/0
	passi lo0
exit

ipv router ospf 200
	router-id 6.6.6.6
exit

access-list 10 permit 172.16.60.0 0.0.0.255
ip nat pool PAT 192.168.56.6 192.168.56.6 netmask 255.255.255.0
ip nat inside source list 10 pool PAT overload
 

